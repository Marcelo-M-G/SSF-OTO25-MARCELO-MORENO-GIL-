4384, 423.5100245986168)\n","(9, 121.75345779049474, 146.10498041151044)\n","(10, -34.43861189536756, 40.043714036243585)\n","... total iteraciones: 21\n","\n","x=100.0\n","Serie directa (iter, suma, error relativo):\n","(1, 1.0, 0.1596638229046939)\n","(2, -4999.000000000004, 5798.159450700569)\n","(3, 4161667.6666666754, 4826134.4359855335)\n","(4, -1384727221.222223, 1605818064.0427568)\n","(5, 246631145794.65195, 286009217378.591)\n","(6, -27310688078191.49, 31671216942914.19)\n","(7, 2060365010708639.8, 2389330764897451.0)\n","(8, -1.1264709096658827e+17, 1.3063275614940656e+17)\n","(9, 4.666830241420834e+18, 5.41195419861332e+18)\n","(10, -1.5152523944444337e+20, 1.757183384406923e+20)\n","... total iteraciones: 2000\n","Computación parcial (iter, suma, error relativo):\n","(1, 1.0, 0.1596638229046939)\n","(2, -4999.0, 5798.159450700565)\n","(3, 4161667.6666666665, 4826134.435985523)\n","(4, -1384727221.222222, 1605818064.0427556)\n","(5, 246631145794.6508, 286009217378.58966)\n","(6, -27310688078191.24, 31671216942913.9)\n","(7, 2060365010708618.8, 2389330764897426.5)\n","(8, -1.1264709096658862e+17, 1.3063275614940698e+17)\n","(9, 4.666830241420797e+18, 5.411954198613277e+18)\n","(10, -1.5152523944444146e+20, 1.757183384406901e+20)\n","... total iteraciones: 2000\n"]},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}},{"output_type":"display_data","data":{"text/plain":["<Figure size 640x480 with 1 Axes>"],"image/png":"\n"},"metadata":{}}],"source":["import math\n","from math import lgamma\n","import pandas as pd\n","import matplotlib.pyplot as plt\n","\n","#para n polinomios usamos:\n","def poly_naive(A, x):\n","    p = 0\n","    for i, a in enumerate(A):\n","        p += (x ** i) * a\n","    return p\n","\n","#usando el Método iterativo: acumula las potencias de x en cada paso.\n","def poly_iter(A, x):\n","    p = 0\n","    xn = 1\n","    for a in A:\n","        p += xn * a\n","        xn *= x\n","    return p\n","\n","#usando el Método de Horner\n","def poly_horner(A, x):\n","    p = A[-1]\n","    i = len(A) - 2\n","    while i >= 0:\n","        p = p * x + A[i]\n","        i -= 1\n","    return p\n","\n","# Ejemplos de polinomios\n","poly_examples = [\n","    ([-1, 2, -6, 2], 3),                 # 2x^3 - 6x^2 + 2x - 1 en x=3\n","    ([1.5, -3.2, 0.0, 4.75, -2.1], 1.2), # polinomio con coeficientes mixtos\n","    ([0, 0, 0, 5], -2.0),                # 5x^3 en x=-2\n","]\n","\n","print(\"\\n=== Evaluación de polinomios ===\")\n","for A, x in poly_examples:\n","  # Evaluamos el polinomio con los tres métodos\n","    v1 = poly_naive(A, x)\n","    v2 = poly_iter(A, x)\n","    v3 = poly_horner(A, x)\n","    print(f\"coef={A}, x={x}\")\n","    print(f\"  Naive: {v1}, Iter: {v2}, Horner: {v3}\\n\")\n","\n","\n","\n","\n","\n","\n","# cos(x) por serie de Taylor\n","\n","\n","def cos_series_direct(x, tol, max_terms=2000):\n","    \"\"\"\n","    Aproximación directa de cos(x) usando la serie de Taylor:\n","    cos(x) = sum_{k=0}^∞ (-1)^k * x^(2k)/(2k)!\n","    Cada término se calcula desde cero usando logaritmos y gamma\n","    para evitar overflow en factoriales grandes.\n","    \"\"\"\n","    rows = []  # almacena (iteración, suma parcial, error relativo)\n","    s = 0.0    # suma parcial\n","    true = math.cos(x)  # valor real para comparar\n","    sign = 1.0\n","    for k in range(max_terms):\n","        # cálculo estable con logaritmos\n","        if x == 0 and k > 0:\n","            mag = 0.0\n","        else:\n","            logabs = (0.0 if (x == 0 and k == 0) else\n","                      (2*k*math.log(abs(x)) - lgamma(2*k + 1.0)))\n","            mag = math.exp(logabs) if (x != 0 or k == 0) else 0.0\n","        term = sign * mag\n","        s += term\n","        # error relativo respecto al valor real\n","        rel_err = abs(s - true) / (abs(true) if true != 0 else float('inf'))\n","        rows.append((k+1, s, rel_err))\n","        if rel_err < tol:  # criterio de parada\n","            break\n","        sign = -sign\n","    return rows\n","\n","def cos_series_partial(x, tol, max_terms=2000):\n","    rows = []\n","    s = 0.0\n","    true = math.cos(x)\n","    term = 1.0 # primer término (k=0)\n","    k = 0\n","    while k < max_terms:\n","        s += term\n","        rel_err = abs(s - true) / (abs(true) if true != 0 else float('inf'))\n","        rows.append((k+1, s, rel_err))\n","        if rel_err < tol:\n","            break\n","        k += 1\n","        # recurrencia para el siguiente término\n","        term = -term * (x*x) / ((2*k-1)*(2*k))\n","    return rows\n","\n","\n","\n","\n","\n","#Función para mostrar tablas de convergencia\n","\n","def show_table(rows, metodo, tol):\n","    df = pd.DataFrame(rows, columns=[\"x\", \"iteración\", \"suma\", \"error relativo\"])\n","    print(f\"\\nTabla de convergencia ({metodo}), tol={tol}, x={rows[0][0]}\")\n","    print(df.head(10))  # muestra primeras 10 filas\n","    return df\n","\n","\n","\n","\n","\n","\n","# 3 Probar cos(x) con tolerancias y varios valores de x\n","\n","\n","x_values = [1e-6, 0.1, 1.0, 5.0, 10.0, 100.0]  # valores de x a probar\n","tolerances = [1e-4, 1e-8]  # tolerancias de error relativo\n","\n","for tol in tolerances:\n","    print(f\"\\n=== cos(x) con tolerancia {tol} ===\")\n","    for x in x_values:\n","        print(f\"\\nx={x}\")\n","        direct = cos_series_direct(x, tol, max_terms=2000)\n","        partial = cos_series_partial(x, tol, max_terms=2000)\n","        print(\"Serie directa (iter, suma, error relativo):\")\n","        for row in direct[:10]:\n","            print(row)\n","        print(\"... total iteraciones:\", len(direct))\n","        print(\"Computación parcial (iter, suma, error relativo):\")\n","        for row in partial[:10]:\n","            print(row)\n","        print(\"... total iteraciones:\", len(partial))\n","\n","\n","\n","\n","\n","\n","# 4 Graficar error vs iteración para x=10 y x=100\n","\n","\n","def plot_error(rows, label):\n","    # Extrae iteraciones y errores de la lista rows y los grafica\n","    iters = [r[0] for r in rows]\n","    errs = [r[2] for r in rows]\n","    plt.plot(iters, errs, label=label)\n","\n","for x in [10.0, 100.0]:\n","    plt.figure()\n","    for tol in tolerances:\n","        # Calcula errores con ambos métodos\n","        d = cos_series_direct(x, tol, max_terms=2000)\n","        p = cos_series_partial(x, tol, max_terms=2000)\n","        plot_error(d, f\"Directa tol={tol}\")\n","        plot_error(p, f\"Parcial tol={tol}\")\n","    # Configuración de la gráfica\n","    plt.yscale(\"log\")  # escala logarítmica para ver órdenes de magnitud\n","    plt.xlabel(\"Iteración\")\n","    plt.ylabel(\"Error relativo\")\n","    plt.title(f\"cos(x) error vs iteración (x={x})\")\n","    # Líneas horizontales para marcar tolerancias\n","    plt.axhline(y=tolerances[0], color=\"gray\", linestyle=\"--\", label=\"tol=1e-4\")\n","    plt.axhline(y=tolerances[1], color=\"black\", linestyle=\"--\", label=\"tol=1e-8\")\n","    plt.legend()\n","    plt.show()\n","\n","\n"]}]}